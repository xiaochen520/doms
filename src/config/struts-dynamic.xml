<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <!-- 系统Action -->
	<package name="dynamic" extends="struts-default">
		<!-- 登陆Action -->
		<action name="sagd_*" class="com.echo.action.SagdAction" method="{1}">
			<result name="success" type="dispatcher">/main.jsp</result>
			<result name="fail" type="dispatcher">/index.jsp</result>
			<result name="input">/index.jsp</result>
		</action>
	</package>
</struts>
