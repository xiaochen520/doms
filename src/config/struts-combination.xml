		<action name="u1sqdt_*" class="com.echo.action.U1sqdtAction" method="{1}"></action>
		
		<!--稀油注输联合站（油区） -->
		<action name="xyyqdt_*" class="com.echo.action.XyyqdtAction" method="{1}"></action>
		<!--稀油注输联合站（水区） -->
		<action name="xysqdt_*" class="com.echo.action.XysqdtAction" method="{1}"></action>
		<!--2号稠油联合处理站（分线计量） -->
		<action name="u2fxjldt_*" class="com.echo.action.U2fxjldtAction" method="{1}"></action>
		
		<!--稠油联合处理站（分线计量） -->
		<action name="fxjlrb_*" class="com.echo.action.fxjlrbAction" method="{1}"></action>
		
		<!--夏18-36注水井日报 -->
		<action name="xiarb_*" class="com.echo.action.xiarbAction" method="{1}"></action>
		<!--夏次高压注水井日报 -->
		<action name="xcgyrb_*" class="com.echo.action.xcgyrbAction" method="{1}"></action>
		<!--2#转油站日报表 -->
		<action name="u2zyzrb_*" class="com.echo.action.U2zyzrbAction" method="{1}"></action>
