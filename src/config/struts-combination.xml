		<action name="xzyzhgrb_*" class="com.echo.action.XzyzhgrbAction" method="{1}"></action>
		
		
		
		<!--2号稠油联合处理站（水区） -->
		<action name="u2sqdt_*" class="com.echo.action.U2sqdtAction" method="{1}"></action>
		<!--2号稠油联合处理站（油区） -->
		<action name="u2yqdt_*" class="com.echo.action.U2yqdtAction" method="{1}"></action>
