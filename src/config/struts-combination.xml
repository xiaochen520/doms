		<action name="sagd_*" class="com.echo.action.SagdAction" method="{1}"></action>
		<!--供气联合站 -->
		<action name="gqlhz_*" class="com.echo.action.GQLHZZHAction" method="{1}"></action>
		<!--采出注入审核统计 -->
		<action name="cczr_*" class="com.echo.action.CCZRSHAction" method="{1}"></action>
		
		<!-- 一号稠油联合处理站水二区罐液位及流量报表 -->
		<action name="*_*" class="com.echo.action.{1}Action" method="{2}"></action>

		<!--过热锅炉周报 -->
		<action name="Grglzb_*" class="com.echo.action.GrglzbAction" method="{1}"></action>
	
		<!--1号稠油联合处理站分线计量日报表 -->
		<action name="cyfx1_*" class="com.echo.action.cyfx1Action" method="{1}"></action>

		<!--2号稠油联合处理站分线计量日报表 -->
		<action name="cyfx2_*" class="com.echo.action.cyfx2Action" method="{1}"></action>
		
		<!--稀油联合处理站分线计量日报表 -->
		<action name="xyfx3_*" class="com.echo.action.xyfx3Action" method="{1}"></action>
		
		<!--1号稠油联合处理站日生产量 -->
		<action name="cyfxsc1_*" class="com.echo.action.cyfxsc1Action" method="{1}"></action>
		
		<!--2号稠油联合处理站日生产量 -->
		<action name="cyfxsc2_*" class="com.echo.action.cyfxsc2Action" method="{1}"></action>
		
		<!--2号稠稀油联合处理站日生产量 -->
		<action name="xyfxsc3_*" class="com.echo.action.xyfxsc3Action" method="{1}"></action>
		
		
		<!--2号稠稀油联合分线计量班报-->
		<action name="fxjl2cy_*" class="com.echo.action.fxjl2cyAction" method="{1}"></action>
		
		
		<!-- 2#软化水站综合日报Action -->
		<action name="u2rhs_*" class="com.echo.action.u2rhsAction" method="{1}"></action>
