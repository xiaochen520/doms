<?xml version="1.0" encoding="UTF-8"?>
<!-- 单幅功图显示配置界面 -->
<applet>
	<table>vwht</table>
	<objectField>UNIT_NAME</objectField>
	<timeField>CJSJ</timeField>
	<dataField>TGI_DIAGRAMS</dataField>
	<width>400</width>
	<height>350</height>
	<title>地面功图</title>
	<titleColor>#000000</titleColor>
	<titleFontSize>12</titleFontSize>
	<titleFontStyle>0</titleFontStyle><!--0:无特殊样式，1:粗体，2:斜体，3:粗体＋斜体-->
	<titleFont>宋体</titleFont>
	<chartClosed>true</chartClosed>
	<chartLineColors>
		<color>#0000FF</color>
		<color>#FF0000</color>
	</chartLineColors>
	<chartLineWidth>1</chartLineWidth>
	<bgColor>#FFFFFF</bgColor>
	<axesLineColor>#000000</axesLineColor>
	<axesLineWidth>1</axesLineWidth>
	<gridLineColor>#999999</gridLineColor>
	<gridLineWidth>1</gridLineWidth>
	<axesFontColor>#000000</axesFontColor>
	<axesFontSize>12</axesFontSize>
	<axesFont>宋体</axesFont>
	<aideTextFontColor>#000000</aideTextFontColor>
	<aideTextFontSize>12</aideTextFontSize>
	<aideTextFont>宋体</aideTextFont>
	<fixChartWHScale>0</fixChartWHScale>
	<minChartWidth>320</minChartWidth>
	<minChartHeiht>300</minChartHeiht>
	<axesX>
		<unit>(m)</unit>
		<tagNum>6</tagNum>
		<showGrid>true</showGrid>
	</axesX>
	<axesY>
		<unit>(KN)</unit>
		<tagNum>6</tagNum>
		<showGrid>true</showGrid>
		<blankScale>10</blankScale>
		<maxLimit>140</maxLimit>
		<minLimit>-40</minLimit>
		<referenceLines>
				<!-- 
			<line>
				<field>P13</field>
				<color>#FF0000</color>
				<width>1</width>
				<desc>FMU</desc>
			</line>
			<line>
				<field>P14</field>
				<color>#FF0000</color>
				<width>1</width>
				<desc>FMD</desc>
			</line>
			 -->
		</referenceLines>
	</axesY>
	<aideTexts>
		<text>
			<prefix>冲程:</prefix>
			<field>STROKE</field>
			<suffix>m</suffix>
		</text>
		<text>
			<prefix>冲次:</prefix>
			<field>JIG_FREQUENCY</field>
			<suffix>次/分钟</suffix>
		</text>
		<text>
			<prefix>--</prefix>
			<field>--</field>
			<suffix>--</suffix>
		</text>
		<text>
			<prefix>最小负荷:</prefix>
			<field>MINIMUM_LOAD</field>
			<suffix>KN</suffix>
		</text>
		<text>
			<prefix>最大负荷:</prefix>
			<field>MAXIMUM_LOAD</field>
			<suffix>KN</suffix>
		</text>
	</aideTexts>
	<markingPointNoFields>
		<field>INDICATOR_DIAGRAMS_POINTS</field>
		<field>INDICATOR_DIAGRAMS_POINTS</field>
		<field>INDICATOR_DIAGRAMS_POINTS</field>
		<field>INDICATOR_DIAGRAMS_POINTS</field>
	</markingPointNoFields>
	<dataFieldEx>
		<field>MAXIMUM_LOAD</field>
	</dataFieldEx>
</applet>
