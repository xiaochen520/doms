<?xml version="1.0" encoding="UTF-8"?>
<!-- 饼图显示配置界面 -->
<applet>
	<form>
		<bgColor>#FFFFFF</bgColor>
		<minDiameter>30</minDiameter>
		<legendPos>bottom</legendPos><!--1:top,2:bottom,3:left,4:right-->
		<width>350</width>
		<height>300</height>
		<pieChartWHScale>1.1</pieChartWHScale>
		<title>
			<Font>宋体</Font>
			<FontSize>12</FontSize>
			<FontColor>#000000</FontColor>
		</title>
		<aideTitle>
			<Font>宋体</Font>
			<FontSize>12</FontSize>
			<FontColor>#000000</FontColor>
		</aideTitle>
		<legendText>
			<Font>宋体</Font>
			<FontSize>12</FontSize>
			<FontColor>#000000</FontColor>
		</legendText>
		<line>
			<Width>1</Width>
			<Color>#000000</Color>
		</line>
		<fillColors>
			<color>#FF0000</color>
			<color>#00FF00</color>
			<color>#0000FF</color>
			<color>#FF00FF</color>
			<color>#00FFFF</color>
			<color>#FFFF00</color>
		</fillColors>
	</form>
	<datas>
		<title>泵效分析结果图</title>
		<aideTitle></aideTitle>
		<desc></desc>
		<unit>%</unit>
		<table>PC_RD_INDICATOR_RESULT_T</table>
		<objectField>UNIT_NAME</objectField>
		<timeField>CJSJ</timeField>
		<dataFields>
			<data>
				<desc>泵效</desc>
				<field>EFFICIENCY_SYSTEM</field>
			</data>
			<data>
				<desc>漏失损失</desc>
				<field>LOSS_STROKE_DODWALL</field>
			</data>
			<data>
				<desc>冲程损失</desc>
				<field>LOSS_DESCE_DODWALL</field>
			</data>
			<data>
				<desc>气体损失</desc>
				<field>CRUDE_OIL_SHRINKAGE</field>
			</data>
			<data>
				<desc>容积损失</desc>
				<field>ROOFBOLT_STROKE_LOSS</field>
			</data>
		</dataFields>
	</datas>

</applet>
