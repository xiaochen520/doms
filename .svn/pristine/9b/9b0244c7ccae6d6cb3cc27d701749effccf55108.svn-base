package com.echo.applet;

/**
 * 创建Applet所需的参数文本，为Applet的辅助工具接口
 * @author wcq
 *
 */
public interface IChartAideTool
{
	/**
	 * 创建Applet界面的格式参数
	 * @return 格式参数文本
	 */
	public String createFormPara();
	
	/**
	 * 创建Applet界面所需的数据参数
	 * @return 数据参数文本
	 */
	public String createDataPara(String strWhere);
	
	/**
	 * 传递用户所需的格式文件名字
	 * @param formName
	 */
	public void setFormXml(String formName);
}
