<?xml version="1.0" encoding="UTF-8"?>
<!-- 单幅功图显示配置界面 -->
<applet>
	<table>vw2070</table>
	<objectField>p01</objectField>
	<timeField>p02</timeField>
	<dataField>p08</dataField>
	<width>260</width>
	<height>210</height>
	<title>地面功图</title>
	<titleColor>#0000FF</titleColor>
	<titleFontSize>12</titleFontSize>
	<titleFont>宋体</titleFont>
	<titleFontStyle>0</titleFontStyle><!--0:无特殊样式，1:粗体，2:斜体，3:粗体＋斜体-->
	<chartClosed>true</chartClosed>
	<chartLineColors>
		<color>#008000</color>
		<color>#007CC8</color>
		<color>#0000CD</color>
		<color>#AFEEEE</color>
		<color>#FFA500</color>
		<color>#0000FF</color>
		<color>#8A2BE2</color>
		<color>#A52A2A</color>
		<color>#6495ED</color>
		<color>#FFF8DC</color>
		<color>#8B008B</color>
	</chartLineColors>
	<chartLineWidth>2</chartLineWidth>
	<bgColor>#FFFFFF</bgColor>
	<axesLineColor>#000000</axesLineColor>
	<axesLineWidth>1</axesLineWidth>
	<gridLineColor>#999999</gridLineColor>
	<gridLineWidth>1</gridLineWidth>
	<axesFontColor>#000000</axesFontColor>
	<axesFontSize>12</axesFontSize>
	<axesFont>宋体</axesFont>
	<aideTextFontColor>#000000</aideTextFontColor>
	<aideTextFontSize>12</aideTextFontSize>
	<aideTextFont>宋体</aideTextFont>
	<fixChartWHScale>0</fixChartWHScale>
	<minChartWidth>100</minChartWidth>
	<minChartHeight>62.5</minChartHeight>
	<axesX>
		<unit>(m)</unit>
		<tagNum>6</tagNum>
		<showGrid>true</showGrid>
	</axesX>
	<axesY>
		<unit>(KN)</unit>
		<tagNum>6</tagNum>
		<showGrid>true</showGrid>
		<blankScale>10</blankScale>
		<maxLimit>140</maxLimit>
		<minLimit>0</minLimit>
		<referenceLines>
			<!-- 
			<line>
				<field>P13</field>
				<color>#FF0000</color>
				<width>1</width>
				<desc>FMU</desc>
			</line>
			<line>
				<field>P14</field>
				<color>#FF0000</color>
				<width>1</width>
				<desc>FMD</desc>
			</line>
			 -->
		</referenceLines>
	</axesY>
	<aideTexts>
		<text>
			<prefix>冲程:</prefix>
			<field>P03</field>
			<suffix>m</suffix>
		</text>
		<text>
			<prefix>冲次:</prefix>
			<field>P04</field>
			<suffix>次/分钟</suffix>
		</text>
		<text>
			<prefix>--</prefix>
			<field>--</field>
			<suffix>--</suffix>
		</text>
		<text>
			<prefix>最小负荷:</prefix>
			<field>P06</field>
			<suffix>KN</suffix>
		</text>
		<text>
			<prefix>最大负荷:</prefix>
			<field>P05</field>
			<suffix>KN</suffix>
		</text>
	</aideTexts>
	<markingPointNoFields>
		<field>P09</field>
		<field>P09</field>
		<field>P09</field>
		<field>P09</field>
	</markingPointNoFields>
	<dataFieldEx>
		<field>p05</field>
	</dataFieldEx>
</applet>
